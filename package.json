{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate && cp static/* docs/",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start dev": "npm run dev",
    "deploy": "npm run generate && npm run move-dist && npm run commit-and-push",
    "move-dist": "rimraf docs && mv dist docs",
    "commit-and-push": "git add . && git commit -m 'Deploy to GitHub Pages' && git push"
  },
  "devDependencies": {
    "@iconify/tailwind": "^1.1.3",
    "@nuxt/content": "^2.13.2",
    "@nuxt/image": "^1.8.0",
    "@nuxtjs/i18n": "^8.5.5",
    "@nuxtjs/seo": "^2.0.0-rc.23",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "nuxt": "^3.13.2",
    "nuxt-icon": "^0.6.8",
    "postcss": "^8.4.32",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.0",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.22",
    "@heroicons/vue": "^2.1.5",
    "marked": "^15.0.3"
  }
}
